<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Shinys_Ultimate_Mod_Manager.App"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="Styles/AppDefaultStyles.axaml"></StyleInclude>
    </Application.Styles>
             
             <Application.Resources>
                 <SolidColorBrush x:Key="PrimaryBackground">#000000</SolidColorBrush>
                 <LinearGradientBrush x:Key="PrimaryBackgroundGradient" StartPoint="0%, 0%" EndPoint="100%, 0%">
                     <GradientStop Offset="0" Color="#616161"></GradientStop>
                     <GradientStop Offset="1" Color="#000000"></GradientStop>
                 </LinearGradientBrush>
             </Application.Resources>
</Application>