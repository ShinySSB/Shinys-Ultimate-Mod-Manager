<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Shinys_Ultimate_Mod_Manager.App"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->
    
    
             
    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="Styles/AppDefaultStyles.axaml"></StyleInclude>
    </Application.Styles>
             
             <Application.Resources>
                 <SolidColorBrush x:Key="PrimaryBackground">#212121</SolidColorBrush>
                 <LinearGradientBrush x:Key="SidebarBackground" StartPoint="0%, 0%" EndPoint="0%, 100%">
                     <GradientStop Offset="0" Color="#212121"></GradientStop>
                     <GradientStop Offset="1" Color="#393939"></GradientStop>
                 </LinearGradientBrush>
             </Application.Resources>
</Application>